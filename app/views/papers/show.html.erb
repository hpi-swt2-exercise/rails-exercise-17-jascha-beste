<P>
  <strong>title:</strong>
  <%= @paper.title %>
</P>
<P>
  <strong>Venue:</strong>
  <%= @paper.venue %>
</P>

<p>
  <strong>Year:</strong>
  <%= @paper.year %>
</p>

<h2>Authors</h2>
<% @paper.authors.each do |author| %>
    <p>
      <strong>Author:</strong>
      <%= author.name %>
    </p>
<% end %>